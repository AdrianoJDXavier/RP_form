{

    "manifest_version": 2,
    "name": "RP_form",
    "version": "1.2",
    "description": "Extensão para preenchimento automático de formulários",

    "permissions": [
        "webRequest",
        "tabs",
        "http://*/*",
        "https://*/*",
        "declarativeContent"
    ],

    "browser_action": {
        "default_icon": "button/button3.png",
        "default_title": "RP_form",
        "default_popup": "popup/rp_form.html"
    },

    "web_accessible_resources": [
        "button/button3.png",
        "ckeditor/ckeditor.js",
        "button/start.ico"
    ],

    "content_scripts": [{
        "js": ["/ckeditor/ckeditor.js", "/content_scripts/rp_form_script.js", "/jquery/jquery-3.5.1.js"],
        "matches": ["https://*/*", "http://*/*"],
        "run_at": "document_end"
    }]

}